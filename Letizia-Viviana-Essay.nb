f2ffca697a75"],

Cell[TextData[{
 "Let ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["X", "i"], "=", 
    RowBox[{"1", " ", "or", " ", "0"}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "fde0a5d1-24b8-4a3b-8320-fdd99a8a03be"],
 " according to the ",
 StyleBox["i",
  FontSlant->"Italic"],
 "th outcome is a success or a failure and let ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["S", "n"], "=", 
    RowBox[{
     RowBox[{
      RowBox[{
       SubscriptBox["X", "1"], "+", 
       SubscriptBox["X", "2"], "+"}], "..."}], "+", 
     SubscriptBox["X", "n"]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "660aa5b2-b495-49b4-89a3-6f916be89dc6"],
 " be the number of success on ",
 StyleBox["n ",
  FontSlant->"Italic"],
 "trials. The probability distribution of ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["S", "n"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "d23ba0e4-05b7-4a45-82b2-c1f648995d11"],
 " is the Binomial Distribution P(k)=  ",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", GridBox[{
      {"n"},
      {"k"}
     }], ")"}], TraditionalForm]], "InlineMath",ExpressionUUID->
  "a4a1898f-6b75-4c4c-bc56-f1031fc25ef8"],
 " ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox[
    RowBox[{
     SuperscriptBox["p", "k"], "(", 
     RowBox[{"1", "-", "p"}], ")"}], 
    RowBox[{"n", "-", "k"}]], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "eaeca364-0034-451e-8017-f657f36ef403"],
 ": ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["S", "n"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "af5f8984-7382-474a-a2c5-4abddd517700"],
 " \[Distributed]BinomialDistribution[n,p]. "
}], "Text",
 CellChangeTimes->{{3.7354837220663652`*^9, 3.735483751473709*^9}, {
   3.735486262485011*^9, 3.735486288116446*^9}, {3.735486386471116*^9, 
